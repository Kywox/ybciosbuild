[sub_resource type="GDScript" id="GDScript_wrhrq"]
resource_name = "battle"
script/source = "extends Node

# For menu control
var posm: int = 1
var texture_FIGHT: Texture  = preload(\"res://ressources/PARTY/FIGHT.png\")
var texture_FIGHTh: Texture  = preload(\"res://ressources/PARTY/FIGHThover.png\")
var texture_ACT: Texture  = preload(\"res://ressources/PARTY/ACT.png\")
var texture_ACTh: Texture  = preload(\"res://ressources/PARTY/ACThover.png\")
var texture_MERCY: Texture  = preload(\"res://ressources/PARTY/MERCY.png\")
var texture_MERCYh: Texture = preload(\"res://ressources/PARTY/MERCYhover.png\")
var state: int = 0
var dialogue: int = 0

# Gameplay
var fight = 0
var BKlife = 100
var mercy = false

func _ready() -> void:
	$wait.start()

func _on_wait_timeout() -> void:
	$buttons/anim.play(\"enter\")

func _process(delta: float) -> void:
	if BKlife <= 0:
		get_tree().change_scene_to_file(\"res://ressources/PARTY/scenes/basementP3fight.tscn\")
	
	if $state.text == \"menu\":
		# INPUTS
		if Input.is_action_just_pressed(\"undertalebattle-left\"):
			if state == 0:
				$move.play()
				posm += -1
		if Input.is_action_just_pressed(\"undertalebattle-right\"):
			if state == 0:
				$move.play()
				posm += 1
		if Input.is_action_just_pressed(\"undertalebattle-interact\"):
			if state == 0:
				if posm == 1:
					$move.play()
					$buttons/MenuController2.play(\"fightgo\")
					$buttons/MenuController.play(\"fightshow\")
					$buttons/anim.play(\"stop\")
					state = 1
				if posm == 2:
					$move.play()
					$buttons/MenuController.play(\"showactions\")
					state = 2
					posm = 1
					$buttons/FIGHT.texture = texture_FIGHT
					$buttons/ACT.texture = texture_ACTh
					$buttons/MERCY.texture = texture_MERCY
				if posm == 3:
					$mercy.play()
					if mercy == false:
						$buttons/text.text = \"rien ne se passe...\"
						$buttons/MenuController2.play(\"textshow\")
						$buttons/anim.play(\"stop\")
						state = 4
					else:
						$buttons/text.text = \"Tu épargne boykisser...\"
						$buttons/MenuController2.play(\"textshow\")
						$buttons/anim.play(\"stop\")
						state = 5
			elif state == 1:
				if fight == 0:
					$attack.play()
					$buttons/MenuController2.stop()
					$buttons/MenuController.play(\"fighthide\")
					$buttons/MenuController2.play(\"textshow\")
					$buttons/text.text = \"YOU MISSED! Boykisser : \" + str(BKlife)
					state = 4
				if fight == 1:
					$attack.play()
					$buttons/MenuController2.stop()
					$buttons/MenuController.play(\"fighthide\")
					$buttons/MenuController2.play(\"textshow\")
					BKlife += -5
					$buttons/text.text = \"BAD! Boykisser : \" + str(BKlife)
					state = 4
				if fight == 2:
					$attack.play()
					$buttons/MenuController2.stop()
					$buttons/MenuController.play(\"fighthide\")
					$buttons/MenuController2.play(\"textshow\")
					BKlife += -10
					$buttons/text.text = \"GOOD! Boykisser : \" + str(BKlife)
					state = 4
				if fight == 3:
					$attack.play()
					$buttons/MenuController2.stop()
					$buttons/MenuController.play(\"fighthide\")
					$buttons/MenuController2.play(\"textshow\")
					BKlife += -15
					$buttons/text.text = \"PERFECT! Boykisser : \" + str(BKlife)
					state = 4
			elif state == 2:
				if posm == 1: # demande de l'aide
					$act.play()
					$buttons/text.text = \"Personne ne repond...\"
					$buttons/anim.play(\"stop\")
					state = 4
				if posm == 2: # fuir
					$act.play()
					$buttons/text.text = \"Tu ne peux pas fuir...\"
					$buttons/anim.play(\"stop\")
					state = 4
				if posm == 3: # insulter
					$act.play()
					$buttons/text.text = \"Il est pas content...\"
					$buttons/anim.play(\"stop\")
					state = 4
				if posm == 4: # discuter
					$act.play()
					$buttons/text.text = \"Tu essaie de lui parler...\"
					if dialogue == 0:
						dialogue = 1
						$buttons/text2/text3.text = \"Pourquoi tu veux pas mon bisou!\"
						$buttons/text2/dianim.play(\"show\")
					elif dialogue == 1:
						dialogue = 2
						$buttons/text2/text3.text = \"Yaxoul! S'il te plaît!\"
						$buttons/text2/dianim.play(\"show\")
					elif dialogue == 2:
						dialogue = 3
						$buttons/text2/text3.text = \"Même kowy en voulait un!\"
						$buttons/text2/dianim.play(\"show\")
					elif dialogue == 3:
						dialogue = 4
						$buttons/text2/text3.text = \"UGGGGGHHH...\"
						$buttons/text2/dianim.play(\"show\")
					elif dialogue == 4:
						dialogue = 5
						$buttons/text2/text3.text = \"tu veux pas mon cadeau?\"
						$buttons/text2/dianim.play(\"show\")
					elif dialogue == 5:
						dialogue = 6
						$buttons/text2/text3.text = \"mais...\"
						$buttons/text2/dianim.play(\"show\")
					elif dialogue == 6:
						dialogue = 7
						$buttons/text2/text3.text = \"S-S'il te plaît...\"
						$buttons/text2/dianim.play(\"show\")
					elif dialogue == 7:
						dialogue = 8
						$buttons/text2/text3.text = \"d-d'accord...\"
						$buttons/text2/dianim.play(\"show\")
						mercy = true
					elif dialogue == 8:
						$buttons/text2/text3.text = \"...\"
						$buttons/text2/dianim.play(\"show\")
					$buttons/anim.play(\"stop\")
					state = 4
				if posm == 5: # bisou
					$act.play()
					$buttons/text.text = \"Tu à décider de te rendre...\"
					$buttons/anim.play(\"stop\")
					state = 69
					$AudioStreamPlayer.stop()
				$buttons/MenuController.play(\"hideactions\")
				$buttons/MenuController2.play(\"textshow\")
			elif state == 4:
				$move.play()
				$buttons/MenuController2.play(\"texthide\")
				$state.text = \"player\"
				RandomAttack()
			elif state == 5:
				$move.play()
				get_tree().change_scene_to_file(\"res://ressources/PARTY/scenes/basementP2mercy.tscn\")
			elif state == 69:
				$move.play()
				$buttons/MenuController2.play(\"texthide\")
				get_tree().change_scene_to_file(\"res://ressources/PARTY/scenes/death2.tscn\")
		if Input.is_action_just_pressed(\"undertalebattle-up\"):
			if state == 2:
				$move.play()
				posm += 1
		if Input.is_action_just_pressed(\"undertalebattle-down\"):
			if state == 2:
				$move.play()
				posm += -1
		if Input.is_action_just_pressed(\"undertalebattle-back\"):
			if state == 2:
				$move.play()
				posm = 2
				state = 0
				$buttons/MenuController.play(\"hideactions\")
		
		# VISUALS
		if state == 0:
			if posm == 0:
				posm = 3
			if posm == 1:
				$buttons/FIGHT.texture = texture_FIGHTh
				$buttons/ACT.texture = texture_ACT
				$buttons/MERCY.texture = texture_MERCY
			if posm == 2:
				$buttons/FIGHT.texture = texture_FIGHT
				$buttons/ACT.texture = texture_ACTh
				$buttons/MERCY.texture = texture_MERCY
			if posm == 3:
				$buttons/FIGHT.texture = texture_FIGHT
				$buttons/ACT.texture = texture_ACT
				$buttons/MERCY.texture = texture_MERCYh
			if posm == 4:
				posm = 1
		if state == 2:
			if posm == 0:
				posm = 5
			if posm == 1:
				$buttons/actions/action1.text = \"> demande de l'aide\"
				$buttons/actions/action1.modulate = Color(1, 1, 0.376)
				$buttons/actions/action2.text = \"fuir\"
				$buttons/actions/action2.modulate = Color(1, 1, 1)
				$buttons/actions/action3.text = \"insulter\"
				$buttons/actions/action3.modulate = Color(1, 1, 1)
				$buttons/actions/action4.text = \"discuter\"
				$buttons/actions/action4.modulate = Color(1, 1, 1)
				$buttons/actions/action5.text = \"bisou\"
				$buttons/actions/action5.modulate = Color(1, 1, 1)
			if posm == 2:
				$buttons/actions/action1.text = \"demande de l'aide\"
				$buttons/actions/action1.modulate = Color(1, 1, 1)
				$buttons/actions/action2.text = \"> fuir\"
				$buttons/actions/action2.modulate = Color(1, 1, 0.376)
				$buttons/actions/action3.text = \"insulter\"
				$buttons/actions/action3.modulate = Color(1, 1, 1)
				$buttons/actions/action4.text = \"discuter\"
				$buttons/actions/action4.modulate = Color(1, 1, 1)
				$buttons/actions/action5.text = \"bisou\"
				$buttons/actions/action5.modulate = Color(1, 1, 1)
			if posm == 3:
				$buttons/actions/action1.text = \"demande de l'aide\"
				$buttons/actions/action1.modulate = Color(1, 1, 1)
				$buttons/actions/action2.text = \"fuir\"
				$buttons/actions/action2.modulate = Color(1, 1, 1)
				$buttons/actions/action3.text = \"> insulter\"
				$buttons/actions/action3.modulate = Color(1, 1, 0.376)
				$buttons/actions/action4.text = \"discuter\"
				$buttons/actions/action4.modulate = Color(1, 1, 1)
				$buttons/actions/action5.text = \"bisou\"
				$buttons/actions/action5.modulate = Color(1, 1, 1)
			if posm == 4:
				$buttons/actions/action1.text = \"demande de l'aide\"
				$buttons/actions/action1.modulate = Color(1, 1, 1)
				$buttons/actions/action2.text = \"fuir\"
				$buttons/actions/action2.modulate = Color(1, 1, 1)
				$buttons/actions/action3.text = \"insulter\"
				$buttons/actions/action3.modulate = Color(1, 1, 1)
				$buttons/actions/action4.text = \"> discuter\"
				$buttons/actions/action4.modulate = Color(1, 1, 0.376)
				$buttons/actions/action5.text = \"bisou\"
				$buttons/actions/action5.modulate = Color(1, 1, 1)
			if posm == 5:
				$buttons/actions/action1.text = \"demande de l'aide\"
				$buttons/actions/action1.modulate = Color(1, 1, 1)
				$buttons/actions/action2.text = \"fuir\"
				$buttons/actions/action2.modulate = Color(1, 1, 1)
				$buttons/actions/action3.text = \"insulter\"
				$buttons/actions/action3.modulate = Color(1, 1, 1)
				$buttons/actions/action4.text = \"discuter\"
				$buttons/actions/action4.modulate = Color(1, 1, 1)
				$buttons/actions/action5.text = \"> bisou\"
				$buttons/actions/action5.modulate = Color(1, 1, 0.376)
			if posm == 6:
				posm = 1

func RandomAttack():
	var random = RandomNumberGenerator.new()
	var number = random.randi_range(1, 3)
	if number == 1:
		$Attacks/attacksanimator.play(\"1\")
	if number == 2:
		$Attacks/attacksanimator.play(\"2\")
	if number == 3:
		$Attacks/attacksanimator.play(\"3\")

func _on_attacksanimator_animation_finished(anim_name: StringName) -> void:
	$state.text = \"menu\"
	$buttons/anim.play(\"enter\")
	state = 0
	posm = 1

func _on_area_2d_area_entered(area: Area2D) -> void:
	if area.is_in_group(\"death\"):
		get_tree().change_scene_to_file(\"res://ressources/PARTY/scenes/death2.tscn\")

func _on_FIGHTGOmiss_animation_finished(anim_name: StringName) -> void:
	if anim_name == \"fightgo\":
		$buttons/MenuController2.stop()
		$buttons/MenuController.play(\"fighthide\")
		$buttons/MenuController2.play(\"textshow\")
		$buttons/text.text = \"YOU MISSED! Boykisser : \" + str(BKlife)
		state = 4

func _on_area_2dfght_area_entered(area: Area2D) -> void:
	if area.is_in_group(\"fight1\"):
		fight = 1
	if area.is_in_group(\"fight2\"):
		fight = 2
	if area.is_in_group(\"fight3\"):
		fight = 3

func _on_area_2dFGHT_area_exited(area: Area2D) -> void:
	if area.is_in_group(\"fight1\"):
		fight = 0
	if area.is_in_group(\"fight2\"):
		fight = 1
	if area.is_in_group(\"fight3\"):
		fight = 2
"

