[sub_resource type="GDScript" id="GDScript_bffhm"]
resource_name = "intro"
script/source = "extends Camera2D

var a: int = 1

func _ready() -> void:
	$Label.text = \" \"
	$Timer.start(5.5)

func _on_timer_timeout() -> void:
	if a == 1:
		a = 2
		$anim.play(\"s\")
		$Label.text = \"ARE           \"
		$Timer.start(0.5)
	elif a == 2:
		a = 3
		$Label.text = \"ARE YOU       \"
		$Timer.start(0.5)
	elif a == 3:
		a = 4
		$Label.text = \"ARE YOU READY?\"
		$Timer.start(1)
	elif a == 4:
		$anim.play(\"start\")
		a = 5
		$Timer.start(2)
	elif a == 5:
		$\"..\"/state.text = \"menu\"
"

