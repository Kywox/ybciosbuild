[sub_resource type="GDScript" id="GDScript_rtr4g"]
resource_name = "plr"
script/source = "extends CharacterBody2D

var input_vector_x: int = 0
var input_vector_y: int = 0
var plrx: int = 0
var plry: int = 0
var speed: int = 20000

func _physics_process(delta: float) -> void:
	if $caninput.text == \"true\":
		if Input.is_action_pressed(\"undertalebattle-slow\"):
			speed = 15000
		else:
			speed = 20000
		
		input_vector_x = Input.get_action_strength(\"1-right\") - Input.get_action_strength(\"1-left\")
		input_vector_y = Input.get_action_strength(\"1-down\") - Input.get_action_strength(\"1-up\")
		if input_vector_y == 0:
			if input_vector_x < 0:
				plrx = -1
				plry = 0
				self.velocity = Vector2(plrx, plry)*delta*speed
			if input_vector_x > 0:
				plrx = 1
				plry = 0
				self.velocity = Vector2(plrx, plry)*delta*speed
			if input_vector_x == 0:
				plrx = 0
				plry = 0
				self.velocity = Vector2(plrx, plry)*delta*speed
		elif input_vector_y > 0:
			if input_vector_x < 0:
				plrx = -1
				plry = 1
				self.velocity = Vector2(plrx, plry)*delta*speed
			if input_vector_x > 0:
				plrx = 1
				plry = 1
				self.velocity = Vector2(plrx, plry)*delta*speed
			if input_vector_x == 0:
				plrx = 0
				plry = 1
				self.velocity = Vector2(plrx, plry)*delta*speed
		elif input_vector_y < 0:
			if input_vector_x < 0:
				plrx = -1
				plry = -1
				self.velocity = Vector2(plrx, plry)*delta*speed
			if input_vector_x > 0:
				plrx = 1
				plry = -1
				self.velocity = Vector2(plrx, plry)*delta*speed
			if input_vector_x == 0:
				plrx = 0
				plry = -1
				self.velocity = Vector2(plrx, plry)*delta*speed
	else:
		plrx = 0
		plry = 0
		self.velocity = Vector2(plrx, plry)*delta*speed
	move_and_slide()
"

